<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        
    
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="PTLCanClient.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
        </sectionGroup>
    </configSections>
    
<applicationSettings>
        <PTLCanClient.Properties.Settings>
          <!--串口名，要小于COM10，如果超过请更改电脑的COM配置，如将COM12改为没有被使用的COM2-->
          <setting name="com" serializeAs="String">
            <value>COM2</value>
          </setting>

          <!--单个灯的颜色(RGB)-->
          <setting name="singleLightColor" serializeAs="String">
            <value>FFFFFF</value>
          </setting>

          <!--多个灯的序号(十进制，英文逗号分隔)-->
          <setting name="multiLights" serializeAs="String">
            <value>1,3</value>
          </setting>

          <!--多个灯的颜色(RGB)-->
          <setting name="multiLightColor" serializeAs="String">
            <value>FFFFFF</value>
          </setting>
          <!--自动清空输入框-->
                <setting name="autoCleanInput" serializeAs="String">
                        <value>True</value>
                </setting>
        </PTLCanClient.Properties.Settings>
    </applicationSettings>
</configuration>